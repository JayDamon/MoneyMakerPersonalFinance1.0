        <ComboBox fx:id="cmbType" editable="true" layoutX="119.0" layoutY="116.0" prefHeight="25.0" prefWidth="199.0" promptText="Type">
            <items>
                <FXCollections fx:factory="observableArrayList" />
            </items>
        </ComboBox>
        <Label layoutX="18.0" layoutY="151.0" text="Account:" />
        <ComboBox fx:id="cmbAccount" editable="true" layoutX="119.0" layoutY="147.0" prefHeight="25.0" prefWidth="199.0" promptText="Account">
