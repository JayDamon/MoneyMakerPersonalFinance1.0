        <Button fx:id="btnExit" layoutX="218.0" layoutY="277.0" onAction="#buttonClickExit" text="Exit" />
        <ComboBox fx:id="cmbPriority" editable="true" layoutX="119.0" layoutY="85.0" prefHeight="25.0" prefWidth="199.0" promptText="Priority">
            <items>
                <FXCollections fx:factory="observableArrayList" />
            </items>
        </ComboBox>
        <Label layoutX="18.0" layoutY="89.0" text="Priority:" />
